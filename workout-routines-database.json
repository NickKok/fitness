        "name": "Neri",
        "age": 47,
        "goals": ["Burn calories", "Reduce visceral fat", "Improve shoulder mobility"],
        "restrictions": ["Early-stage frozen shoulder", "No equipment"],
        "frequency": "3x/week"
      },
      "structure": {
        "warmup": {
          "exercises": [
            {
              "name": "Arm Circles",
              "reps": "10 each direction",
              "notes": "Forward and backward"
            },
            {
              "name": "Cat Cow",
              "reps": 12,
              "notes": "Focus on spinal mobility"
            },
            {
              "name": "March in Place",
              "duration_sec": 60,
              "notes": "Only duration-based exercise in warmup"
            }
          ]
        },
        "circuit": {
          "rounds": 3,
          "exercises": [
            {
              "name": "Squat + Side Leg Raise",
              "reps": 15,
              "focus": ["legs", "glutes", "core"]
            },
            {
              "name": "Mountain Climbers",
              "reps": "20 total (10/side)",
              "focus": ["cardio", "core"]
            },
            {
              "name": "Bird Dog (Dynamic)",
              "reps": 12,
              "focus": ["core", "shoulder mobility"]
            }
          ],
          "rest_between_exercises_sec": 30
        },
        "cooldown": {
          "exercises": [
            {
              "name": "Child's Pose",
              "duration_sec": 60,
              "notes": "Duration specified for stretching"
            },
            {
              "name": "Thread the Needle",
              "reps": "5 each side"
            }
          ]
        }
      },
      "notes": "Modify shoulder movements if pain occurs. Most exercises use reps unless duration is specified."
    },
    {
