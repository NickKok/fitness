          "phase": "main"
        },
        {
          "name": "Quadruped Hip Extensions",
          "reps": "10-15 per leg",
          "focusAreas": [
            "glutes",
            "core"
          ],
          "phase": "main"
        },
        {
          "name": "Static Lunge Pulse",
          "reps": "10-12 per leg",
          "focusAreas": [
            "quads",
            "glutes"
          ],
          "phase": "main"
        },
        {
          "name": "Side Lunges",
          "reps": "10 per side",
          "focusAreas": [
            "adductors",
            "quads"
          ],
          "phase": "main"
