        {
          "name": "Single-Leg Mini Squat Hold",
          "duration_sec": "10-15 per leg",
          "focusAreas": [
            "VMO",
            "ankle stability"
          ],
          "phase": "main",
          "notes": "Add pulses if too easy"
        },
        {
          "name": "Wall Sit",
          "duration_sec": "30-60",
          "focusAreas": [
            "isometric",
            "quads"
          ],
          "phase": "main"
        },
        {
          "name": "Toe-to-Wall Tibialis Raises",
          "reps": 12,
          "focusAreas": [
            "tibialis anterior"
          ],
          "phase": "cooldown"
        },
        {
          "name": "Child's Pose",
          "duration_sec": 45,
          "focusAreas": [
            "stretching",
            "relaxation"
          ],
          "phase": "cooldown",
          "notes": "Focus on deep breathing"
        }
      ],
      "notes": "Perform exercises in order. Modify reps if experiencing knee discomfort. The single-leg focus enhances stability while protecting joints.",
      "circuitRounds": 3,
      "restBetweenRoundsSec": 60
    }
